        <com.googlecode.jmeter.plugins.webdriver.sampler.WebDriverSampler guiclass="com.googlecode.jmeter.plugins.webdriver.sampler.gui.WebDriverSamplerGui" testclass="com.googlecode.jmeter.plugins.webdriver.sampler.WebDriverSampler" testname="LoginPage">
          <stringProp name="WebDriverSampler.script">import org.openqa.selenium.By

def serverName = WDS.vars.get(&quot;server_name&quot;)
WDS.browser.get(serverName)

def currentUrl = WDS.browser.currentUrl
if (!currentUrl.contains(&quot;https://www.saucedemo.com&quot;)) {
    WDS.log.error(&quot;Page not opened!&quot;)
    WDS.sampleResult.successful = false
}</stringProp>
          <stringProp name="WebDriverSampler.parameters"></stringProp>
          <stringProp name="WebDriverSampler.language">groovy</stringProp>
        </com.googlecode.jmeter.plugins.webdriver.sampler.WebDriverSampler>
