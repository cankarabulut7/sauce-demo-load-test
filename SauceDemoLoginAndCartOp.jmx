        <com.googlecode.jmeter.plugins.webdriver.sampler.WebDriverSampler guiclass="com.googlecode.jmeter.plugins.webdriver.sampler.gui.WebDriverSamplerGui" testclass="com.googlecode.jmeter.plugins.webdriver.sampler.WebDriverSampler" testname="Login">
          <stringProp name="WebDriverSampler.script">import org.openqa.selenium.By

def username = WDS.vars.get(&quot;username&quot;)
def password = WDS.vars.get(&quot;password&quot;)

WDS.browser.findElement(By.cssSelector(&quot;[data-test=&apos;username&apos;]&quot;)).sendKeys(&quot;$username&quot;)
WDS.browser.findElement(By.cssSelector(&quot;[data-test=&apos;password&apos;]&quot;)).sendKeys(&quot;$password&quot;)
WDS.browser.findElement(By.cssSelector(&quot;[data-test=&apos;login-button&apos;]&quot;)).click()

def currentUrl = WDS.browser.currentUrl
if (!currentUrl.contains(&quot;inventory&quot;)) {
    WDS.log.error(&quot;Login failed!&quot;)
    WDS.sampleResult.successful = false
}</stringProp>
          <stringProp name="WebDriverSampler.parameters"></stringProp>
          <stringProp name="WebDriverSampler.language">groovy</stringProp>
        </com.googlecode.jmeter.plugins.webdriver.sampler.WebDriverSampler>
        <hashTree/>
        <com.googlecode.jmeter.plugins.webdriver.sampler.WebDriverSampler guiclass="com.googlecode.jmeter.plugins.webdriver.sampler.gui.WebDriverSamplerGui" testclass="com.googlecode.jmeter.plugins.webdriver.sampler.WebDriverSampler" testname="AddToCart">
          <stringProp name="WebDriverSampler.script">import org.openqa.selenium.By

def addToCartBtn = WDS.browser.findElement(org.openqa.selenium.By.cssSelector(&quot;button[data-test=&apos;add-to-cart-sauce-labs-backpack&apos;]&quot;))

addToCartBtn.click()

def badge = WDS.browser.findElement(By.cssSelector(&quot;span[data-test=&apos;shopping-cart-badge&apos;]&quot;))

if (badge.text != &quot;1&quot;) {
    WDS.log.error(&quot;Product could not be add to cart!&quot;)
    WDS.sampleResult.successful = false
} else {
    WDS.log.info(&quot;Product has been added to the cart!&quot;)
}
</stringProp>
          <stringProp name="WebDriverSampler.parameters"></stringProp>
          <stringProp name="WebDriverSampler.language">groovy</stringProp>
        </com.googlecode.jmeter.plugins.webdriver.sampler.WebDriverSampler>
        <hashTree/>
        <com.googlecode.jmeter.plugins.webdriver.sampler.WebDriverSampler guiclass="com.googlecode.jmeter.plugins.webdriver.sampler.gui.WebDriverSamplerGui" testclass="com.googlecode.jmeter.plugins.webdriver.sampler.WebDriverSampler" testname="GoShoppingCart">
          <stringProp name="WebDriverSampler.script">import org.openqa.selenium.By

def cartIcon = WDS.browser.findElement(By.cssSelector(&quot;a[data-test=&apos;shopping-cart-link&apos;]&quot;))
cartIcon.click()

def currentUrl = WDS.browser.currentUrl
if (!currentUrl.contains(&quot;cart&quot;)) {
    WDS.log.error(&quot;Failed!&quot;)
    WDS.sampleResult.successful = false
}
</stringProp>
          <stringProp name="WebDriverSampler.parameters"></stringProp>
          <stringProp name="WebDriverSampler.language">groovy</stringProp>
        </com.googlecode.jmeter.plugins.webdriver.sampler.WebDriverSampler>
